#include<iostream>
using namespace std;

int main()
{
	int age;

	cout<<"Enter the age of candidate: ";
	cin>>age;

	//Candidate is teenager or not
	//>=13 and <=19
	if(age>=13 && age<=19)
	{
		cout<<"Candidate is Teenager"<<endl;
	}
	else
	{
		cout<<"Candidate is not a Teenager"<<endl;
	}


	//condition to check voting eligility
	if(age>=18)
	{
		cout<<"Candidate is eligible for voting"<<endl;
	}
	else
	{
		cout<<"Candidate is not eligible for voting"<<endl;
	}

	return 0;
}
